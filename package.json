{
    "name": "tsp-website",
    "version": "1.0.0",
    "scripts": {
        "wds": "wds",
        "build-zola-local": "zola --config config.local.toml build",
        "build-zola-live": "zola --config config.toml build",
        "build-rollup": "rollup --config",
        "serve": "zola serve --interface 0.0.0.0 --port 2000 --base-url rhea.lan",
        "local-dev": "npm run build-zola-local && zola --config config.local.toml serve && npm run wds",
        "live-builder": "npm run build-zola-live && npm run build-rollup"
    },
    "dependencies": {
        "add-prefetch-link": "^1.0.2"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^11.2.1",
        "@web/dev-server": "^0.1.24",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "rollup": "^2.58.0",
        "rollup-plugin-terser": "^7.0.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TonySpegel/tsp-website.git"
    },
    "author": "Tony Spegel"
}
